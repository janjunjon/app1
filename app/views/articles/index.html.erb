<% provide(:title, '論文一覧') %>
<% provide(:user, "#{current_user.name}さん") %>
<% @articles.each do |article| %>
	<div>
		<h2 class="contents">
			<%= link_to article.title, article_path(article) %>
		</h2>
		<p class="contents"><%= article.abstract %></p>
	</div>
<% end %>
<%= will_paginate %>