<% provide(:title, '論文一覧') %>
<div class="research">
    <%= render 'layouts/research' %>
</div>
<% @articles.each do |article| %>
	<div class="elements">
		<h2 class="contents">
			<%= link_to article.title, article_path(article) %>
		</h2>
		<p class="contents"><%= article.abstract %></p>
	</div>
<% end %>
<%= will_paginate %>